  },
  "homepage": "https://github.com/StefanZott/Launcher#readme",
  "devDependencies": {
    "electron": "^18.0.1",
    "electron-builder": "^22.14.13",
    "electron-packager": "^15.4.0"
  },
  "description": ""
}
